<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>j2ee</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>



	<servlet>
		<servlet-name>Program1</servlet-name>
		<servlet-class>com.j2ee.Program1</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Program1</servlet-name>
		<url-pattern>/callProgram1</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.j2ee.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/callLogin</url-pattern>
	</servlet-mapping>


<servlet>
		<servlet-name>Forward</servlet-name>
		<servlet-class>com.j2ee.Forward</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Forward</servlet-name>
		<url-pattern>/Forward</url-pattern>
	</servlet-mapping>



<servlet>
		<servlet-name>Forward2</servlet-name>
		<servlet-class>com.j2ee.Program4</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Forward2</servlet-name>
		<url-pattern>/Forward</url-pattern>
	</servlet-mapping>




<servlet>
		<servlet-name>sum</servlet-name>
		<servlet-class>com.j2ee.Sum</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sum</servlet-name>
		<url-pattern>/callsum</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>product</servlet-name>
		<servlet-class>com.j2ee.Product</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>product</servlet-name>
		<url-pattern>/callproduct</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>callblock</servlet-name>
		<servlet-class>com.j2ee.block</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>callblock</servlet-name>
		<url-pattern>/block</url-pattern>
	</servlet-mapping>

</web-app>